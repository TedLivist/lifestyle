<h1>Haha Index Page</h1>

<% if user_signed_in? %>
  <h4><%= current_user.username %></h4>
<% end %>

<% top_vote_article = Article.find(Vote.group(:article_id).count.max[0]) %>

<div>
  <div class="top-image">
    <p><%= top_vote_article.title %></p>
  </div>
</div>

<div class="container-box">
  <% Category.all.each do |pr| %>
    <div><%= pr.name %></div>
  <% end %>
</div>